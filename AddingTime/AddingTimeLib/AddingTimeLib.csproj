	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\BDInfo\BDInfoLib.csproj" />
	  <ProjectReference Include="..\DvdNavigatorCrm\DvdNavigatorCrm.csproj" />
	</ItemGroup>

</Project>